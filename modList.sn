
module List:
  class C:
    def Nil()
    def Cons(x, xs)
  end

  let Nil = Nil()

  def append(lst1, lst2):
    match(lst1):
    case([]):
      lst2
    case([x | xs]):
      [x | append(xs, lst2)]
    end
  end

  module Open:
    let C#(++) = append

    export C#(++)
  end

  export C, Nil, Cons, append, Open
end

open List::Open
