module DivToFloat:
  def Int::C#(/)(self, other):
    Float::from_int(self) / Float::from_int(other)
  end
  export Int::C#(/)
end
assert(3 / 2 == 1)
begin:
  open DivToFloat
  assert(3 / 2 == 1.5)
end
assert(3 / 2 == 1)
